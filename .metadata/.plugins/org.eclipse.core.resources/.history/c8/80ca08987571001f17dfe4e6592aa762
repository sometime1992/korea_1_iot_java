package chapter16;

// 학생 클래스 - 기본 클래스
class Student {
	private String name;
	private int score;
	
	public Student(String name, int score) {
		this.name = name;
		this.score = score;
	}
	
	public String getName() {
		return name;
	}
	
	public int getScore() {
		return score;
	}
}

// 인터페이스 정의 - 성적 계산
interface Grading {
	String calculateGrade(Student student); // 추상 메서드
}

public class B_Anonymous02 {
	public static void main(String[] args) {
		Student st1 = new Student("이승아", 85);
		Student st2 = new Student("이도경", 72);
		Student st3 = new Student("이은우", 50);
		Student st4 = new Student("김소빈", 865);
		Student st5 = new Student("현유상", 81);
		
		Grading grading = new Grading() {
			
			@Override
			public String calculateGrade(Student student) {
				// TODO Auto-generated method stub
				return null;
			}
		};
		
		
		
		
		
		
		
		
		
		
		
	}
}
