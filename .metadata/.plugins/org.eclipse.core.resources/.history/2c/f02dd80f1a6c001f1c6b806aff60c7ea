package otherPackage;

// 접근 범위: 제한 X
public class OtherClass {
	// 클래스의 속성
	
	// 어디서든 접근 가능
	public int publicVar = 1;
	
	// 같은 패키지 O, 다른 패키지 - 해당 클래스를 상속받은 자식 클래스만 접근 가능
	protected int protectedVar = 2;
	
	// 같은 패키지 O, 다른 패키지 X
	int defaultVar = 3;
	
	private int privateVar = 4;
	
	// 클래스의 메서드
	public void ex1() {
		
	}
	
	public void ex2() {
		
	}
	
	void ex3() {
		
	}
	
	private void ex4() {
		
	}
	
	
}
