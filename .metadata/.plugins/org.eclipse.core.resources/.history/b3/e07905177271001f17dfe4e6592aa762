package chapter16;

// 익명 클래스
// : 이름이 없는 클래스
// - 주로 일회성 객체를 생성할 때 사용
// - 클래스를 정읳먄서 동시에 그 인스턴스를 생성
// - 추상 클래스나 인터페이스 구현에 사용

// == 기본 형태 ==
// new 추상클래스명 or 인터페이스명() {

// }

abstract class Example {
	abstract void display();
}

// A라는 이름을 가진 클래스
class A extends Example {
	void display() {
		System.out.println("출력합니다.");
	}
}

public class B_Anonymous01 {
	A a1 = new A();
	A a2 = new A();
	A a3 = new A();
}
