package chapter04;

// 클래스의 구조: 필드, 메서드, 생성자
class Car {
	// 클래스의 속성(필드)
	String model; // 모델
	int yaer; // 제조 연도
	
	// 클래스의 행동(메서드)
	// : 클래스 내부에서 정의된 함수, 객체의 행동을 표현
	// >> 클래스의 데이터(필드)를 사용하여 특정 작업을 수행하거나
	//		, 해당 값을 변경 가능, 값을 반환
}

public class Object03 {
	public static void main(String[] args) {
		
	}
}
