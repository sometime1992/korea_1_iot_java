package chapter05;


class Example {
	// (인스턴스) 변수 선언
	// : 데이터 타입이 같은 경우 타입 생략 가능
	int x, y;
	// int x;
	// int y;
	
	// add 메서드
	int add(int x, int y) {
		return x + y;
	}
	
	// multiply 메서드
	int multiply(int x, int y) {
		return x + y;
	}
	
	// 매개변수 X, 반환값 X
	void print99dan() {
		for (int i = 0; i < 8; i++) {
			for (int j = 0; j < 8; j++) {
				System.out.println(j * i + " ");
			}
			System.out.println();
		}
	}
}

